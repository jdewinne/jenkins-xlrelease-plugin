<?xml version='1.0' encoding='UTF-8'?>
<synthetic xsi:schemaLocation="http://www.xebialabs.com/deployit/synthetic synthetic.xsd" xmlns="http://www.xebialabs.com/deployit/synthetic" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <type type="jenkins.BuildJob" extends="xlrelease.PythonScript">
        <property name="jenkinsUrl" category="input" label="Jenkins URL" required="true"/>
        <property name="keyFile" category="input" label="Key File" required="true"/>
        <property name="password" password="true" category="input" required="true"/>
        <property name="jobName" category="input" label="Job Name" required="true"/>
        <property name="parameters" size="large" category="input" label="Parameters to pass" required="false"/>
    </type>



</synthetic>